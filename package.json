{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run compress-assets",
    "build:analyze": "ANALYZE=true npm run build",
    "build:optimized": "tsc && VITE_BUNDLE_VISUALIZE=1 vite build && npm run compress-assets && npm run optimize:all",
    "compress-assets": "cd dist/public && (find assets -name '*.js' -o -name '*.css' | xargs -I {} gzip -k9 \"{}\") && cd ../.. && node scripts/compress.js",
    "compress:test": "node scripts/compression-test.js",
    "compress:fix": "node scripts/compress.js --force",
    "minify:js": "find dist/public -name '*.js' -not -name '*.min.js' | xargs -I{} terser {} -c -m -o {}",
    "minify:js:aggressive": "find dist/public -name '*.js' -not -name '*.min.js' | xargs -I{} terser {} -c 'passes=5,pure_getters=true,unsafe=true,unsafe_math=true,unsafe_arrows=true,drop_console=true' -m 'toplevel=true' -o {}",
    "preview": "vite preview",
    "start": "node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "lighthouse": "npx lighthouse http://localhost:3000 --view --output-path=./lighthouse-report.html",
    "performance:check": "npx web-vitals-reporter --url http://localhost:3000",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:js": "npm run minify:js:aggressive",
    "optimize:compression": "node scripts/compress.js",
    "optimize:bundle": "node scripts/pre-deploy.js",
    "optimize:all": "npm run optimize:images && npm run optimize:js && npm run optimize:compression && npm run optimize:bundle",
    "serve:optimized": "npm run build:optimized && npm run preview",
    "speed-test": "node scripts/speed-test.js",
    "speed-test:prod": "node scripts/speed-test.js https://your-production-url.com",
    "pre-deploy": "npm run build:optimized",
    "analyze:unused-js": "npx vite-bundle-analyzer"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@jridgewell/trace-mapping": "^0.3.25",
